# Private key for deployment and transactions
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_project_id
ARB_RPC_URL=https://arbitrum-sepolia.infura.io/v3/your_project_id

# Arbitrum One mainnet RPC (accepts alias ARBITRUM_RPC_URL)
ARBITRUM_ONE_RPC_URL=https://arbitrum.llamarpc.com
# ARBITRUM_RPC_URL=https://arbitrum.llamarpc.com

# Optional: Fork Arbitrum Sepolia for local development
ARB_RPC_URL_FORK=https://arbitrum-sepolia.infura.io/v3/your_project_id

# API Keys for verification
ARBISCAN_API_KEY=your_arbiscan_api_key_here

# Source RPC for state migration (can be same as SEPOLIA_RPC_URL)
SOURCE_RPC_URL=https://sepolia.infura.io/v3/your_project_id

# Contract addresses for state migration (set after deployment)
L1_COUNTER_ADDRESS=0x...
L2_COUNTER_ADDRESS=0x...

# Replay configuration (optional)
REPLAY_FROM_ADDRESS=0x06395a32ba4c6a468D35E451cbf93b0f07da902b
REPLAY_BLOCKS=10
REPLAY_START_BLOCK=
REPLAY_DRY_RUN=1
REPLAY_JSON_OUT=0
